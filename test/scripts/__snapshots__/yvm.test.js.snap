// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`yvm commands current shows yarn version 1`] = `"1.13.0"`;

exports[`yvm commands help shows all help with --help 1`] = `
"Usage: yvm [options] [command]

Yarn Version Manager

Options:
  -V, --version                       output the version number
  -h, --help                          output usage information

Commands:
  install|i [options] [version]       Install the specified version of Yarn. Uses config file if available.
  uninstall|rm <version>              Uninstall the specified version of Yarn.
  use [version]                       Activate specified Yarn version. Uses config file if available.
  exec [options] [version] [args...]  Execute command using specified Yarn version. Uses config file if available.
  current                             Display current active Yarn version
  list|ls                             List the currently installed versions of Yarn.
  list-remote|ls-remote               List Yarn versions available to install.
  alias [<pattern>]                   Show all aliases matching <pattern>
  alias <name> <version>              Set an alias named <name> pointing to <version>
  unalias [options] <name>            Deletes the alias named <name>
  which [version]                     Display path to Yarn version. Uses config file if available.
  update-self                         Updates yvm to the latest version
  version                             See -> 'yvm --version'
  remove <version>                    See -> 'yvm uninstall'
  set-default <version>               See -> 'yvm alias default <version>'
  get-default-version                 See -> 'yvm alias default'"
`;
