{
  "plugins": ["prettier"],
  "extends": [
    "airbnb/base",
    "prettier"
  ],
  "env": {
        "browser": false,
        "node": true,
        "jest": true,
        "es6": true
  },
  "rules": {
    "prettier/prettier": [
      2,
      {
        "bracketSpacing": true,
        "semi": false,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all"
      }
    ]
  }
}
