{
  "plugins": ["prettier"],
  "extends": [
    "airbnb/base",
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "prettier/prettier": [
      2,
      {
        "bracketSpacing": true,
        "semi": false,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all"
      }
    ]
  }
}
